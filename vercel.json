{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/auth/login",
            "dest": "/auth/login"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
